@model VetPet.Models.SedeClinicaEntity

<h2>Crear Nueva Sede</h2>

<form asp-action="Create" method="post">
    <div class="form-group col-md-4">
        <label asp-for="Nombre">Nombre</label>
        <input asp-for="Nombre" class="form-control" placeholder="Nombre" required>
        <span asp-validation-for="Nombre" class="text-danger"></span>
    </div>
    <div class="form-group col-md-4">
        <label asp-for="Direccion">Direccion</label>
        <input asp-for="Direccion" class="form-control" placeholder="Direccion" required>
        <span asp-validation-for="Direccion" class="text-danger"></span>
    </div>

    <div class="form-group">
        <button type="submit" class="btn btn-success">Guardar</button>
        <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
    </div>
</form>
